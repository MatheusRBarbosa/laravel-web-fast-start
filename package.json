    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "cross-env": "^7.0",
    "husky": "^7.0.0",
    "jquery": "^3.6",
    "laravel-mix": "^5.0.1",
    "lodash": "^4.17.19",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.32.11",
    "sass-loader": "^10.1.1",
    "standard-version": "^9.3.1",
    "sweetalert2": "^11.1.7",
    "vue-template-compiler": "^2.6.14"
  },
